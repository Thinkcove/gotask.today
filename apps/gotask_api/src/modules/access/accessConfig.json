{
  "accesses": [
    {
      "access": "User Management",
      "actions": ["READ", "CREATE", "EDIT", "VIEW", "DELETE"],
      "fields": {
        "read": ["name", "user_id", "status", "roleId", "organization", "projects"],
        "edit": ["name", "status", "roleId", "organization", "projects"]
      }
    },
    {
      "access": "Task Management",
      "actions": ["READ", "CREATE", "EDIT", "VIEW", "DELETE"],
      "fields": {
        "read": [
          "id", "title", "description", "status", "severity", "user_id",
          "user_name", "project_id", "project_name", "due_date",
          "created_on", "updated_on", "estimated_time", "time_spent_total",
          "remaining_time", "variation"
        ],
        "edit": [
          "title", "description", "status", "severity",
          "due_date", "estimated_time", "remaining_time"
        ]
      }
    },
    {
      "access": "Project Management",
      "actions": ["READ", "CREATE", "EDIT", "VIEW", "ASSIGN", "UNASSIGN", "UPDATE"],
      "fields": {
        "read": ["id", "name", "description", "status", "user_id", "organization_id", "createdAt", "updatedAt"],
        "edit": ["name", "description", "status", "user_id", "organization_id"],
        "assign": ["user_id"],
        "unassign": ["user_id"],
        "update": ["name", "description", "status", "user_id", "organization_id"]
      }
    },
    {
      "access": "Access Management",
      "actions": ["READ", "CREATE", "VIEW", "EDIT", "DELETE", "UPDATE"],
      "fields": {
        "read": ["id", "name", "application"],
        "edit": ["name", "application"],
        "update": ["name", "application"]
      }
    },
    {
      "access": "Organization Management",
      "actions": ["READ", "CREATE", "VIEW", "EDIT"],
      "fields": {
        "read": ["id", "name", "address", "mail_id", "mobile_no", "projects", "users"],
        "edit": ["name", "address", "mail_id", "mobile_no", "projects", "users"]
      }
    },
    {
      "access": "Role Management",
      "actions": ["READ", "CREATE", "VIEW", "EDIT", "DELETE", "INVOKE_ACCESS", "REVOKE_ACCESS"],
      "fields": {
        "read": ["id", "name", "access"],
        "edit": ["name", "access"]
      }
    },
    {
      "access": "User Report",
      "actions": ["READ"],
      "fields": {
        "read": ["reportType", "createdAt"]
      }
    }
  ]
}
